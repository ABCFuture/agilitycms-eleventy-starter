{
  "name": "agility-cms-11ty-blog",
  "version": "1.0.0",
  "description": "A blog with 11th with content from Agility CMS",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development eleventy --input=pages",
    "build": "NODE_ENV=production node agility/agility.sync.js sync && eleventy --input=pages",
    "start": "NODE_ENV=development eleventy --input=pages --serve --watch",
    "cms-clear": "node agility/agility.sync.js clear",
    "cms-pull": "NODE_ENV=development node agility/agility.sync.js sync"
  },
  "author": "Joel Varty",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@agility/content-sync": "^0.1.10",
    "dotenv": "^8.2.0",
    "moment": "^2.29.1",
    "truncate-html": "^1.0.3"
  }
}
